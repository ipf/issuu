<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<TCEforms>
			<sheetTitle>Options</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
			<settings.jCode>
				<TCEforms>
					<label>Link for the ISSUU document</label>
					<config>
						<size>32</size>
						<max>1000</max>
						<type>input</type>
						<eval>required</eval>
					</config>
				</TCEforms>
			</settings.jCode>
			<settings.width>
				<TCEforms>
					<label>Width of the document (in px)</label>
					<config>
						<size>4</size>
						<max>4</max>
						<type>input</type>
						<eval>int</eval>
					</config>
				</TCEforms>
			</settings.width>
			<settings.height>
				<TCEforms>
					<label>Height of the document (in px)</label>
					<config>
						<size>4</size>
						<max>4</max>
						<type>input</type>
						<eval>int</eval>
					</config>
				</TCEforms>
			</settings.height>
		</el>
	</ROOT>
</T3DataStructure>